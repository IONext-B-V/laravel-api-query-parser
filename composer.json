{
    "name": "ionext/laravel-api-query-parser",
    "description": "A query parser for REST-APIs based on the laravel framework",
    "keywords": ["laravel", "query parser", "api"],
    "homepage": "https://github.com/ngabor84/lumen-api-query-parser",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Gydo Broos",
            "email": "info@ionext.nl",
            "homepage": "https://ionext.nl",
            "role": "developer"
        }
    ],
    "require": {
    	"php": "^7.2",
        "laravel/framework": "^6.2"
    },
    "require-dev": {
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^8.4",
        "nunomaduro/phpinsights": "^1.11"
    },
    "autoload": {
        "psr-4": {
            "ApiQueryParser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
